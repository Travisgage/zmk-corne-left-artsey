#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        base {
            bindings = <
                /* Row 1 */
                &none &kp A &kp R &kp T &kp S &none
                /* Row 2 */
                &none &kp E &kp Y &kp I &kp O &none
                /* Row 3 */
                &none &none &kp SPC &kp TAB &none &none
            >;
        };
    };
};