#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        base {
            bindings = <
                /* Matrix row 0 — top physical row */
                &kp 1 &kp 2 &kp 3 &kp 4 &kp 5 &kp 6

                /* Matrix row 1 — middle physical row */
                &kp Q &kp W &kp E &kp R &kp T &kp Y
            >;
        };
    };
};

