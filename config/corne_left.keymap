#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        discover {
            bindings = <
                /* Row 0 */
                &kp N1 &kp N2 &kp N3 &kp N4 &kp N5 &kp N6
                /* Row 1 */
                &kp N7 &kp N8 &kp N9 &kp N0 &kp MINUS &kp EQUAL
                /* Row 2 */
                &kp Q  &kp W  &kp E  &kp R  &kp T  &kp Y
            >;
        };
    };
};